{
	"manifest_version": 2,
	"name": "obli",
	"version": "0.0.01",
	"content_scripts": [
		{
			"all_frames": true, 
			"js": ["src/webinterface.js"],
			"matches": ["<all_urls>"],
            "run_at": "document_start"
		}
	],
	"permissions": [
		"tabs",
		"unlimitedStorage",
        "storage"
	],
    "background": {
        "scripts": ["src/background.js"],
        "persistent": true
    },
	"options_page": "src/web/options.html"
}
